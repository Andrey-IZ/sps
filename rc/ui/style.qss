/* **********************        QSpinBox     *******************************/
QSpinBox, QDoubleSpinBox, HexSpinBox {
    border-radius: 3px;
    border: 1px solid blue ;
    background: white;
    selection-background-color: #1777aa;
}
QSpinBox:disabled, QDoubleSpinBox:disabled, HexSpinBox:disabled {
  color: gray;
  background: lightgray;
}
QSpinBox:focus, QSpinBox:hover, QDoubleSpinBox:focus, QDoubleSpinBox:hover, HexSpinBox:focus, HexSpinBox:hover   {
  border: 1px solid blue ;
}

QSpinBox::up-button,  QDoubleSpinBox::up-button, HexSpinBox::up-button {
   subcontrol-origin: border; 
   subcontrol-position: top right; /* position at the top right corner */

   width: 18px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
   height: 18px;
  border-image: url(":/img/ico/sp_up_origin.png") 1;
}

QSpinBox::up-button:hover, QDoubleSpinBox::up-button:hover, HexSpinBox::up-button:hover {
      border-image: url(":/img/ico/sp_up_hover.png");
}

QSpinBox::up-button:pressed, QDoubleSpinBox::up-button:pressed, HexSpinBox::up-button:pressed {
  border-image: url(":/img/ico/sp_up_pressed.png");
}

QSpinBox::up-arrow, QDoubleSpinBox::up-arrow, HexSpinBox::up-arrow {
  /* image: url(":/img/ico/sp_up_origin.png") ; */
    width: 18px;
    height: 18px;
}

QSpinBox::up-button:disabled, 
QDoubleSpinBox::up-button:disabled, HexSpinBox::up-button:disabled { /* off state when value is max */
   border-image: url(":/img/ico/sp_up_disabled.png");
}
QSpinBox::up-arrow:disabled, QSpinBox::up-arrow:off,HexSpinBox::up-arrow:disabled, HexSpinBox::up-arrow:off,
QDoubleSpinBox::up-arrow:disabled, QDoubleSpinBox::up-arrow:off { /* off state when value is max */
/*   image: url(":/img/ico/sp_up_disabled.png") */
 border-image: url(":/img/ico/sp_up_disabled.png");
}

QSpinBox::down-button, QDoubleSpinBox::down-button, HexSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: top left; /* position at bottom right corner */
    height: 18px;
    width: 18px;
    
    border-image: url(":/img/ico/sp_down_origin.png");
}

QSpinBox::down-button:hover, QDoubleSpinBox::down-button:hover, HexSpinBox::down-button:hover
{
  border-image: url(":/img/ico/sp_down_hover.png");
}

QSpinBox::down-button:pressed, QDoubleSpinBox::down-button:pressed, HexSpinBox::down-button:pressed {
  border-image: url(":/img/ico/sp_down_pressed.png");
}

QSpinBox::down-arrow, QDoubleSpinBox::down-arrow, HexSpinBox::down-arrow {
/*     image: url(:/images/down_arrow.png); */
    width: 18px;
    height: 18px;
}
QSpinBox::down-button:disabled, QDoubleSpinBox::down-button:disabled, HexSpinBox::down-button:disabled{
  border-image: url(":/img/ico/sp_down_disabled.png");  
}

QSpinBox::down-arrow:disabled,QSpinBox::down-arrow:off, QDoubleSpinBox::down-arrow:disabled,
QDoubleSpinBox::down-arrow:off, HexSpinBox::down-arrow:off { /* off state when value in min */
  border-image: url(":/img/ico/sp_down_disabled.png");
}
/* **********************        QComboBox     *******************************/
QComboBox {
    padding: 1px 1px 1px 3px;
    min-width: 2em;
}

QComboBox:!editable:disabled {
    color: #fff;
    border-style: solid;
    border: 2px outset white;
    background-color: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #eee, stop: 1 gray);
}

QComboBox:editable {
/*     background: red; */
}

QComboBox:!editable:hover {
   border: 3px solid blue;
   font-weight: bold; 
}
QComboBox:!editable, QComboBox::drop-down:editable {
    border: 2px solid blue;
    font-weight: bold; 
    background-color: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #fff, stop: 1 #88aacc);
    color: black;
}

/* QComboBox gets the "on" state when the popup is open */
QComboBox:!editable:on, QComboBox::drop-down:editable:on {
    font-weight: 100; 
    color: white;
    border: 2px solid blue;
    background: #356ccc;
}

QComboBox:on { /* shift the text when the popup opens */
    padding-top: 4px;
    padding-left: 5px;
    background: blue;
}

QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 17px;

    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid; /* just a single line */
    border-top-right-radius: 3px; /* same radius as the QComboBox */
    border-bottom-right-radius: 3px;
}

/*QComboBox::down-arrow:disabled {
  border-image: url(":/img/ico/combo_dropdown_down_disabled.png"); 
}*/
  
QComboBox::down-arrow {
    /*image: url(/usr/share/icons/crystalsvg/16x16/actions/1downarrow.png);*/
    border-image: url(":/img/ico/combo_dropdown_down.png"); 
}

QComboBox::down-arrow:on { /* shift the arrow when popup is open */
/*    top: 1px;
    left: 1px;*/
    border-image: url(":/img/ico/combo_dropdown_up.png"); 
}

