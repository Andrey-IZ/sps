/* **********************        QLineEdit     *******************************/
QLineEdit {
     border: 2px solid blue;
     border-radius: 8px;
     background: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #fff, stop: 1 #E1EEff);
     selection-background-color: darkblue;
}
QLineEdit:focus {
  background: white;
}

QLineEdit:hover {
