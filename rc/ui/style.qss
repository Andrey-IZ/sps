QSlider#sliderSwitcher::groove:horizontal {
border: 0px solid #000;
height: 14px;
background: #9AD9EB;
border-radius: 2px;
}

QSlider#sliderSwitcher::handle:horizontal {
background: qradialgradient(cx:0, cy:0, radius: 1.4,
                fx:0.5, fy:0.5, stop:0 white, stop:1 gray);
/*background:  qradialgradient(spread:repeat, cx:0.5, cy:0.5, radius:0.077, fx:0.5, fy:0.5,
   stop:0 rgba(160, 169, 166, 255),
   stop:0.5 rgba(255, 255, 250, 255), 
   stop:1 rgba(170, 169, 170, 250));*/
border: 1px solid #000000;
width: 24px;
margin: -6px 0;
border-radius: 12px;
}

QSlider#sliderSwitcher::sub-page:qlineargradient {
/*background:  qlineargradient(x1:0, y1:0, x2:1, y2:1,    
    stop:0 #DDF, stop:1 #0000FF);*/
background: qlineargradient(x1:0, y1:0, x2:1, y2:1,    
    stop:0 #88BBDD, stop:1 snow);
border-top-right-radius: 0px;
border-bottom-right-radius: 0px;
border-top-left-radius: 9px;
border-bottom-left-radius: 9px;
border-radius: 4px;
border: 1px solid #000;
}

QSlider#sliderSwitcher::add-page:qlineargradient {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1,    
    stop:0 snow, stop:1 #88BBDD);
border-top-right-radius: 9px;
border-bottom-right-radius: 9px;
border-top-left-radius: 0px;
border-bottom-left-radius: 0px;
border-radius: 4px;
border: 1px solid #000;
}

QSlider#sliderSwitcher::sub-page:horizontal:disabled {
background: #eee;
border-color: #999;
}

QSlider#sliderSwitcher::add-page:horizontal:disabled {
background: #eee;
border-color: #999;
}

QSlider#sliderSwitcher::handle:horizontal:disabled {
background: qradialgradient(cx:0, cy:0, radius: 1.4,
                fx:0.5, fy:0.5, stop:0 #eee, stop:1 #eee);
width: 24px;
margin: -6px 0;
border-radius: 12px;
}
/*********************      QSlider      **********************/
QSlider::groove:horizontal {
border: 1px solid #bbb;
background: white;
height: 10px;
border-radius: 4px;
}

QSlider::sub-page:horizontal {
background: qlineargradient(x1: 0, y1: 0,    x2: 0, y2: 1,
    stop: 0 #66e, stop: 1 #bbf);
background: qlineargradient(x1: 0, y1: 0.2, x2: 1, y2: 1,
    stop: 0 #bbf, stop: 1 #55f);
border: 1px solid #777;
height: 10px;
border-radius: 4px;
}

QSlider::add-page:horizontal {
background: #fff;
border: 1px solid #777;
height: 10px;
border-radius: 4px;
}

QSlider::handle:horizontal {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
    stop:0 #eee, stop:1 #ccc);
border: 1px solid #777;
width: 13px;
margin-top: -2px;
margin-bottom: -2px;
border-radius: 4px;
}

QSlider::handle:horizontal:hover {
background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
    stop:0 #fff, stop:1 #ddd);
border: 1px solid #444;
border-radius: 4px;
}

QSlider::sub-page:horizontal:disabled {
background: #bbb;
border-color: #999;
}

QSlider::add-page:horizontal:disabled {
background: #eee;
border-color: #999;
}

QSlider::handle:horizontal:disabled {
background: #eee;
border: 1px solid #aaa;
border-radius: 4px;
}

/***************      QScrollBar      ************************/
  QScrollBar:vertical {
     background: #e4e4e4;
     border-top-right-radius: 4px;
     border-bottom-right-radius: 4px;
     width: 12px;
     margin: 0px;
 }
 QScrollBar::handle:vertical {
    background-color: qlineargradient( x1: 0, y1: 0, x2: 1, y2: 0,
                                    stop: 0 #4287ff, stop: 1 #356ccc);
     border-radius: 4px;
     min-height: 20px;
     margin: 0px 2px 0px 2px;
 }
 QScrollBar::add-line:vertical {
     background: none;
     height: 0px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:vertical {
     background: none;
     height: 0px;
     subcontrol-position: left;
     subcontrol-origin: margin;
 }
 /** horizontal ***/
  QScrollBar:horizontal {
     background: #e4e4e4;
     border-bottom-left-radius: 4px;
     border-top-left-radius: 4px;
     height: 12px;
     margin: 0px;
 }
 QScrollBar::handle:horizontal {
    background-color: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                    stop: 1 #4287ff, stop: 0 #356ccc);
     border-radius: 4px;
     min-width: 20px;
     margin: 2px 0px 2px 0px;
 }
 QScrollBar::add-line:horizontal {
     background: none;
     width: 0px;
     subcontrol-position: left;
     subcontrol-origin: margin;
 }
 QScrollBar::sub-line:horizontal {
     background: none;
     width: 0px;
     subcontrol-position: right;
     subcontrol-origin: margin;
 }
 /*************        QToolBar       ******************************/
 QToolBar {
    color: #fff;
    font-size: 14px;
    border-radius: 2px;
    border: 1px solid #3873d9;
    background-color: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #fefefe, stop: 1 #aaaaaa);
    spacing: 3px; /* spacing between items in the tool bar */
}

QToolBar::handle {
    image: url(handle.png);
}
/***************      QToolButton         **********************************/
QToolButton { /* all types of tool button */
    border: 2px solid #fefefe;
    border-radius: 6px;
    color: #355553;
    background-color: qlineargradient( x1: 0, y1: 0, x2: 0, y2: 1,
                                        stop: 0 #fefefe, stop: 1 #aaaaaa);
    /*color: yellow;*/
    /*background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #f6f7fa, stop: 1 #dadbde);*/
}
/**/
QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 20px; /* make way for the popup button */
}

QToolButton:pressed {
    border-radius: 6px;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 #dadbde, stop: 1 #f6f7fa);
}
QToolButton:checked:hover {
    border: 2px solid #11cc11;
    color: black;
    /*border-radius: 6px;*/
}

QToolButton:checked:pressed {
    border: 1px solid  yellow;   
    color: black;
}

QToolButton:hover {
    border: 1px solid red;
    color: black;
    /*background: red;*/
}

QToolButton:checked {
    border: 2px solid #22ff55;
    border-radius: 6px;
    color: black;
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 white, stop: 1 #aaaaaa);
}

/* the subcontrols below are used only in the MenuButtonPopup mode */
QToolButton::menu-button {
    border: 2px solid gray;
    border-top-right-radius: 6px;
    border-bottom-right-radius: 6px;
    /* 16px width + 4px for border = 20px allocated above */
    width: 16px;
}

QToolButton::menu-arrow {
    image: url(downarrow.png);
}

QToolButton::menu-arrow:open {
    top: 1px; left: 1px; /* shift it a bit */
}
